       program       
   pcdb_admin    false    4            ]           1255    16436    add_override_type()    FUNCTION        CREATE FUNCTION program.add_override_type() RETURNS trigger
    LANGUAGE plpgsql SECURITY DEFINER
    AS $$
BEGIN
IF(NEW."policyID" IS NULL AND NEW."serviceID" IS NULL)
THEN
NEW."overrideTypeID" = 1;
ELSIF(NEW."policyID" IS NULL AND NEW."serviceID" IS NOT NULL)
THEN
NEW."overrideTypeID" = 2;
ELSIF(NEW."policyID" IS NOT NULL AND NEW."serviceID" IS NULL)
THEN
NEW."overrideTypeID" = 3;
ELSIF(NEW."policyID" IS NOT NULL AND NEW."serviceID" IS NOT NULL)
THEN
NEW."overrideTypeID" = 4;
END IF;
RETURN NEW;
END;
$$;
 +   DROP FUNCTION program.add_override_type();
